{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "values": "<DVC_METRIC_DATA>"
  },
  "title": "<DVC_METRIC_TITLE>",
  "width": 600,
  "height": 400,
  "mark": {
    "type": "bar",
    "tooltip": true
  },
  "encoding": {
    "x": {
      "field": "<DVC_METRIC_X>",
      "type": "nominal",
      "title": "Model",
      "axis": {
        "labelAngle": -45
      }
    },
    "y": {
      "field": "<DVC_METRIC_Y>",
      "type": "quantitative",
      "title": "Score",
      "scale": {
        "domain": [0.9, 1.0]
      },
      "axis": {
        "grid": true,
        "gridOpacity": 0.3
      }
    },
    "color": {
      "field": "<DVC_METRIC_X>",
      "type": "nominal",
      "legend": null,
      "scale": {
        "scheme": "category20"
      }
    }
  },
  "config": {
    "view": {
      "strokeWidth": 0
    },
    "axis": {
      "labelFontSize": 12,
      "titleFontSize": 14
    },
    "title": {
      "fontSize": 16,
      "fontWeight": "bold"
    }
  }
}