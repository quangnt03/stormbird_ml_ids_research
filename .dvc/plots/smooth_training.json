{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "data": {
    "values": "<DVC_METRIC_DATA>"
  },
  "title": "<DVC_METRIC_TITLE>",
  "width": 600,
  "height": 400,
  "mark": {
    "type": "line",
    "point": false,
    "interpolate": "monotone",
    "strokeWidth": 2
  },
  "encoding": {
    "x": {
      "field": "<DVC_METRIC_X>",
      "type": "quantitative",
      "title": "Iteration",
      "scale": {"zero": false}
    },
    "y": {
      "field": "<DVC_METRIC_Y>",
      "type": "quantitative",
      "title": "Loss",
      "scale": {"zero": false}
    },
    "color": {
      "field": "rev",
      "type": "nominal",
      "title": "Experiment",
      "legend": {
        "orient": "bottom"
      }
    }
  }
}